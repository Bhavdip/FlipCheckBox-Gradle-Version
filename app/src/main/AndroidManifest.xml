<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.franlopez.demoflipcheckbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activites.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.CustomAcceptStyleGridActivity" />
        <activity android:name=".activites.CustomStyleGridActivity" />
        <activity android:name=".activites.CustomViewStyleActivity" />
        <activity android:name=".activites.NormalStyleActivity" />
    </application>

</manifest>